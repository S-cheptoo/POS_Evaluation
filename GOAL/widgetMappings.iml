<!-- ============================================================= -->
<!-- widgetMappings.iml                                            -->
<!-- ============================================================= -->
<!-- WARNING: This file must be located at project root.           -->
<!--          This file is used in all GML files of the project    -->
<!-- ============================================================= -->
<!-- In GML files, all the widgets are identified by an "<idname>" -->
<!-- element. They allow to manage interactions between the        -->
<!-- application business part (written in "C" code) and           -->
<!-- the screen resource.                                          -->
<!-- The "idname" and "idval" attributes are used to build in      -->
<!-- include file (IncGen/widgeMappings.h) to develop the          -->
<!-- application business part in "C".                             -->
<!-- For each <widgetMapping> tag, the "#define idname idval" line -->
<!-- is generated in "widgeMappings.h" file.                       -->
<!-- That is the reason why "idname" must follow the same ANSI     -->
<!-- rules used in a "#define".                                    -->
<!-- "idval" is the string representation of an integer.           -->
<!-- Only ids declared in "idname" attributes can be used id GML   -->
<!-- files.                                                        -->
<widgetMappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.ingenico.com/schemas/widgetmappings/1_0">
    <widgetMapping idname="DATE_SCREEN_EDIT_DATE" idval="1"/>
    <widgetMapping idname="DATE_SCREEN_BUTTON_OK" idval="2"/>
    <widgetMapping idname="GENERIC_BUTTON_CANCEL" idval="3"/>
</widgetMappings>
